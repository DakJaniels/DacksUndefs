    "Lua.runtime.special": {
        "require": "require"
    },
    "Lua.workspace.maxPreload": 20000,
    "Lua.workspace.preloadFileSize": 20000,
    "Lua.workspace.checkThirdParty": true,
    "Lua.type.castNumberToInteger": true,
    "Lua.typeFormat.config": {
        "format_line": "false"
    },
    "Lua.misc.parameters": [
        "--develop=true"
    ]
